{
    "name": "slang-script-text-mate",
    "displayName": "vscode-slang-script-language",
    "description": "Slang Grammar",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "slang",
                "aliases": [
                    "Slang Script",
                    "slang"
                ],
                "extensions": [
                    ".sl"
                ],
                "icon": {
                    "light": "./assets/slang.png",
                    "dark": "./assets/slang.png"
                },
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "slang",
                "scopeName": "source.SlangScript",
                "path": "./syntaxes/slang.tmLanguage.json"
            }
        ]
    }
}