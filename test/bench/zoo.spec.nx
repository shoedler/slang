cls Zoo {
  ctor -> {
    this.aardvark = 1
    this.baboon   = 1
    this.cat      = 1
    this.donkey   = 1
    this.elephant = 1
    this.fox      = 1
  }
  fn ant    -> { ret this.aardvark; }
  fn banana -> { ret this.baboon; }
  fn tuna   -> { ret this.cat; }
  fn hay    -> { ret this.donkey; }
  fn grass  -> { ret this.elephant; }
  fn mouse  -> { ret this.fox; }
}

let zoo = Zoo()
let sum = 0
let start = clock()

while (sum < 100000000) {
  sum = sum + zoo.ant()
            + zoo.banana()
            + zoo.tuna()
            + zoo.hay()
            + zoo.grass()
            + zoo.mouse()
}

print clock() - start
print sum
